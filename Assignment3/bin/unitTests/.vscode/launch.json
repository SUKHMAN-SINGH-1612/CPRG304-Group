{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "java",
        "name": "Run BSTreeTest JUnit",
        "request": "launch",
        "mainClass": "org.junit.runner.JUnitCore",
        "args": [
          "unitTests.BSTreeTest"
